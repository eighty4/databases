    "development",
    "devtool",
    "docker",
    "elasticsearch",
    "mongodb",
    "mssql",
    "mysql",
    "postgres",
    "postgresql",
    "schema",
    "scylladb",
    "sqlserver"
  ],
  "license": "BSD-3-Clause",
  "type": "module",
  "bin": "lib/bin.js",
  "main": "lib/index.js",
